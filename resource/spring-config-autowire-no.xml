<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<bean id="customer" class="com.stundent.spring.dependency.autowire.Customer">
		<property name="person" ref="person" />
	</bean>

	<bean id="person" class="com.stundent.spring.dependency.autowire.Person">
		<property name="personId" value="P001" />
		<property name="personName" value="Rakesh" />
	</bean>
</beans>